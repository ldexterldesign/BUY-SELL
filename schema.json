{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "alias": {
        "s": "http://schema.org/"
    },
    "definitions": {
        "disambiguatingDescription": {
            "$comment": "Use schema.org's disambiguatingDescription keyword to avoid conflict with json-schema.org's description keyword",
            "$id": "#disambiguatingDescription",
            "s:additionalType": "s:Text",
            "s:sameAs": "s:disambiguatingDescription",
            "type": "string"
        },
        "email": {
            "$id": "#email",
            "s:additionalType": "s:Text",
            "s:sameAs": "s:email",
            "type": "string"
        },
        "location": {
            "$id": "#location",
            "properties": {
                "address": {
                    "properties": {
                        "addressCountry": {
                            "s:additionalType": "s:Text",
                            "s:sameAs": "s:addressCountry",
                            "type": "string"
                        },
                        "addressLocality": {
                            "s:additionalType": "s:Text",
                            "s:sameAs": "s:addressLocality",
                            "type": "string"
                        },
                        "addressRegion": {
                            "s:additionalType": "s:Text",
                            "s:sameAs": "s:addressRegion",
                            "type": "string"
                        },
                        "postalCode": {
                            "s:additionalType": "s:Text",
                            "s:sameAs": "s:postalCode",
                            "type": "string"
                        },
                        "streetAddress": {
                            "s:additionalType": "s:Text",
                            "s:sameAs": "s:streetAddress",
                            "type": "string"
                        }
                    },
                    "s:additionalType": "s:PostalAddress",
                    "s:sameAs": "s:address",
                    "type": "object"
                },
                "geo": {
                    "properties": {
                        "latitude": {
                            "s:additionalType": "s:Number",
                            "s:sameAs": "s:latitude",
                            "type": "number"
                        },
                        "longitude": {
                            "s:additionalType": "s:Number",
                            "s:sameAs": "s:longitude",
                            "type": "number"
                        }
                    },
                    "s:additionalType": "s:GeoCoordinates",
                    "s:sameAs": "s:geo",
                    "type": "object"
                }
            },
            "s:additionalType": "s:Place",
            "s:sameAs": "s:location",
            "type": "object"
        },
        "name": {
            "$id": "#name",
            "s:additionalType": "s:Text",
            "s:sameAs": "s:name",
            "type": "string"
        },
        "telephone": {
            "$id": "#telephone",
            "s:additionalType": "s:Text",
            "s:sameAs": "s:telephone",
            "type": "string"
        },
        "url": {
            "$id": "#url",
            "s:additionalType": "s:URL",
            "s:sameAs": "s:url",
            "type": "string"
        }
    },
    "description": "Schema uses http://schema.org/TradeAction as a base type then branches to http://schema.org/BuyAction or http://schema.org/SellAction type depending on whether property instance is http://schema.org/buyer or http://schema.org/seller",
    "properties": {
        "buyer": {
            "$ref": "#person",
            "description": "The participant that bought the thing",
            "s:additionalType": "https://schema.org/SellAction",
            "s:sameAs": "s:buyer"
        },
        "dateCreated": {
            "format": "date",
            "s:additionalType": "https://schema.org/Date",
            "s:sameAs": "https://schema.org/dateCreated",
            "type": "string"
        },
        "priceSpecification": {
            "properties": {
                "deliveryChargeSpecification": {
                    "properties": {
                        "eligibleRegion": {
                            "description": "Do use ISO 3166-1 alpha-3 three-letter country codes http://en.wikipedia.org/wiki/ISO_3166-1_alpha-3",
                            "examples": [
                                "mlt",
                                "gbr"
                            ],
                            "items": {
                                "type": "string"
                            },
                            "s:additionalType": "s:Text",
                            "s:sameAs": "s:eligibleRegion",
                            "type": "array"
                        },
                        "price": {
                            "$ref": "#price"
                        }
                    },
                    "s:additionalType": "s:DeliveryChargeSpecification",
                    "type": "object"
                },
                "price": {
                    "$id": "#price",
                    "s:additionalType": "s:Number",
                    "s:sameAs": "s:price",
                    "type": "number"
                },
                "priceCurrency": {
                    "examples": [
                        "gbp",
                        "eur"
                    ],
                    "s:additionalType": "s:Text",
                    "s:sameAs": "s:priceCurrency",
                    "type": "string"
                },
                "totalPrice": {
                    "s:additionalType": "s:Number",
                    "s:sameAs": "s:totalPrice",
                    "type": "number"
                },
                "totalPriceUnit": {
                    "description": "totalPrice / unitPriceSpecification.referenceQuantity",
                    "type": "number"
                },
                "unitPriceSpecification": {
                    "properties": {
                        "referenceQuantity": {
                            "s:additionalType": "s:QuantitativeValue",
                            "value": "number"
                        }
                    },
                    "s:additionalType": "s:UnitPriceSpecification",
                    "type": "object"
                },
                "valueAddedTaxIncluded": {
                    "s:additionalType": "s:Boolean",
                    "s:sameAs": "s:valueAddedTaxIncluded",
                    "type": "boolean"
                }
            },
            "s:additionalType": "s:PriceSpecification",
            "type": "object"
        },
        "product": {
            "properties": {
                "brand": {
                    "properties": {
                        "name": {
                            "s:additionalType": "s:Text",
                            "type": "string"
                        },
                        "url": {
                            "$ref": "#url"
                        }
                    },
                    "s:additionalType": "s:Brand",
                    "s:sameAs": "s:brand",
                    "type": "object"
                },
                "identifier": {
                    "properties": {
                        "gtin": {
                            "s:additionalType": "s:Text",
                            "s:sameAs": "s:gtin",
                            "type": "string"
                        },
                        "productID": {
                            "s:additionalType": "s:Text",
                            "s:sameAs": "s:productID",
                            "type": "string"
                        }
                    },
                    "s:additionalType": "https://schema.org/PropertyValue",
                    "s:sameAs": "https://schema.org/identifier",
                    "type": "object"
                },
                "manufacturer": {
                    "$ref": "#organization",
                    "s:sameAs": "s:manufacturer"
                },
                "model": {
                    "s:additionalType": "s:Text",
                    "s:sameAs": "s:model",
                    "type": "string"
                },
                "mpn": {
                    "s:additionalType": "s:Text",
                    "s:sameAs": "s:mpn",
                    "type": "string"
                },
                "name": {
                    "$ref": "#name"
                },
                "size": {
                    "s:additionalType": "https://schema.org/Text",
                    "s:sameAs": "https://schema.org/size",
                    "type": "string"
                }
            },
            "s:additionalType": "s:Product",
            "type": "object"
        },
        "seller": {
            "anyOf": [
                {
                    "if": {
                        "properties": {
                            "agentType": {
                                "const": "person"
                            }
                        }
                    },
                    "then": {
                        "properties": {
                            "agent": {
                                "$id": "#person",
                                "affiliation": {
                                    "$ref": "#organization",
                                    "s:sameAs": "s:affiliation"
                                },
                                "disambiguatingDescription": {
                                    "$ref": "#disambiguatingDescription"
                                },
                                "email": {
                                    "$ref": "#email"
                                },
                                "location": {
                                    "$ref": "#location"
                                },
                                "name": {
                                    "$ref": "#name"
                                },
                                "s:additionalType": "s:Person",
                                "s:sameAs": "s:agent",
                                "telephone": {
                                    "$ref": "#telephone"
                                },
                                "type": "object",
                                "url": {
                                    "$ref": "#url"
                                }
                            }
                        }
                    }
                },
                {
                    "if": {
                        "properties": {
                            "agentType": {
                                "const": "organization"
                            }
                        }
                    },
                    "then": {
                        "properties": {
                            "agent": {
                                "$id": "#organization",
                                "disambiguatingDescription": {
                                    "$ref": "#disambiguatingDescription"
                                },
                                "email": {
                                    "$ref": "#email"
                                },
                                "ethicsPolicy": {
                                    "s:additionalType": "s:URL",
                                    "s:sameAs": "s:ethicsPolicy",
                                    "type": "string"
                                },
                                "location": {
                                    "$ref": "#location"
                                },
                                "name": {
                                    "$ref": "#name"
                                },
                                "parentOrganization": {
                                    "$ref": "#organization",
                                    "s:sameAs": "s:parentOrganization"
                                },
                                "s:additionalType": "s:Organization",
                                "s:sameAs": "s:agent",
                                "telephone": {
                                    "$ref": "#telephone"
                                },
                                "type": "object",
                                "url": {
                                    "$ref": "#url"
                                }
                            }
                        }
                    }
                }
            ],
            "description": "The participant that sold the thing",
            "properties": {
                "agentType": {
                    "enum": [
                        "person",
                        "organization"
                    ],
                    "type": "string"
                }
            },
            "s:additionalType": "https://schema.org/BuyAction",
            "s:sameAs": "s:seller",
            "type": "object"
        },
        "service": {
            "s:additionalType": "s:Service",
            "type": "object"
        }
    },
    "title": "Structured data for buying/selling something",
    "type": "object"
}